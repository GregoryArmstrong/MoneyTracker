<h1>Categories Index</h1>

<%= link_to "New Category", new_admin_category_path %>

<ul>
  <% @categories.each do |category| %>
  <li><%= category.name %>
  <%= link_to("Edit Category", edit_admin_category_path(category)) %>
  <%= link_to("Delete Category", admin_category_path(category), method: :delete) %></li>
<% end %>
</ul>

<%= react_component('AdminCategoriesIndex') %>
